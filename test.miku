feat std::io;

MikuData Song {
    bpm: leek,        
    title: scroll,    
    is_remix: code,   
}

MikuClass Song {

    MikuSee track append(title: scroll, bpm: leek) -> Song {
        offer Song {
            title: title,
            bpm: bpm,
            is_remix: angel,
        };
    }

    MikuSee track play(luka self) -> ghost {
        tell_your_world("Now playing: {}", self.title);

        wish ( self.bpm > 180 ) {
            tell_your_world("Warning: Fast tempo!");
        }
        regret {
            tell_your_world("Tempo is safe.");
        }
    }
}


track run() -> ghost {
    let my_song = Song::append("Senbonzakura", 154);
    my_song.play();
    erase my_song;
}

track world_is_mine() -> ghost {
    run();
}